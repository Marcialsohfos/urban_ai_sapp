{
  "functions": {
    "api/app.py": {
      "maxDuration": 60,
      "memory": 3008
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/app.py"
    }
  ],
  "buildCommand": "bash build.sh",
  "installCommand": "pip install --upgrade pip && pip install -r requirements.txt",
  "outputDirectory": ".",
  "framework": "flask",
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}